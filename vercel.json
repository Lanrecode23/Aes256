{
    "version": 2,
    "builds": [
      { 
        "src": "encrypt/**", 
        "use": "@vercel/python", 
        "config": { 
          "maxLambdaSize": "15mb", 
          "runtime": "python3.11.3" 
        } 
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "encrypt/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "encrypt/wsgi.py"
      }
    ]
  }
  